{
  "private": true,
  "name": "generation-latinx-n",
  "productName": "generation-latinx-n",
  "description": "My exceptional Vue project",
  "version": "0.0.2",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_PATH=. poi",
    "build": "cross-env NODE_PATH=. poi build",
    "report": "poi build --bundle-report",
    "serve": "serve dist --single",
    "test": "tyu",
    "test:ci": "nyc ava src",
    "deploy": "cp -a dist/. public/ ",
    "env:copy": "cross-env POI_APP_READONLY_KEY=$POI_APP_READONLY_KEY cp src/services/config.js.example src/services/config.js",
    "postinstall": "npm run env:copy && npm run build && npm run deploy && echo 'Client built!'"
  },
  "author": {
    "name": "Rene M. Paccha",
    "email": "1162669+renemarcelo@users.noreply.github.com"
  },
  "dependencies": {
    "airtable": "^0.5.7",
    "axios": "^0.18.0",
    "cross-env": "^5.2.0",
    "gsap": "^2.0.2",
    "materialize-css": "^1.0.0-rc.2",
    "normalize.css": "^7.0.0",
    "object-assign": "^4.1.1",
    "offline-plugin": "4.9.1",
    "poi": "9.6.13",
    "poi-preset-bundle-report": "2.0.2",
    "poi-preset-offline": "9.0.3",
    "promise-polyfill": "7.1.2",
    "serve": "^10.1.1",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.0.0",
    "@vue/test-utils": "^1.0.0-beta.27",
    "ava": "1.0.0-rc.2",
    "browser-env": "^3.2.5",
    "node-sass": "^4.10.0",
    "nyc": "^13.1.0",
    "require-extension-hooks": "^0.3.3",
    "require-extension-hooks-babel": "^0.1.1",
    "require-extension-hooks-vue": "^1.1.0",
    "sass-loader": "^7.1.0",
    "tyu": "^1.0.6",
    "vue-devtools": "^5.0.0-beta.1"
  },
  "ava": {
    "require": [
      "./test/helpers/setup.js",
      "@babel/polyfill"
    ]
  },
  "jest": {
    "unmockedModulePathPatterns": [
      "object-assign"
    ]
  },
  "env": {
    "test": {
      "plugins": [
        [
          "istanbul",
          {
            "exclude": [
              "**/*.test.js"
            ]
          }
        ]
      ]
    }
  }
}
