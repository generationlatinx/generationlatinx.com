{
  "private": true,
  "name": "generation-latinx-n",
  "productName": "generation-latinx-n",
  "description": "My exceptional Vue project",
  "version": "0.0.2",
  "license": "MIT",
  "scripts": {
    "dev": "poi",
    "build": "poi build",
    "report": "poi build --bundle-report",
    "serve": "serve dist --single",
    "test": "ava",
    "test:ci": "npx nyc ava src",
    "deploy": "cp -a dist/. public/ ",
    "postinstall": "npm run build && npm run deploy && echo 'Client built!'"
  },
  "author": {
    "name": "Rene M. Paccha",
    "email": "1162669+renemarcelo@users.noreply.github.com"
  },
  "dependencies": {
    "airtable": "^0.5.7",
    "axios": "^0.18.0",
    "gsap": "^2.0.2",
    "materialize-css": "^1.0.0-rc.2",
    "normalize.css": "^7.0.0",
    "object-assign": "^4.1.1",
    "offline-plugin": "4.9.1",
    "poi": "9.6.13",
    "poi-preset-bundle-report": "2.0.2",
    "poi-preset-offline": "9.0.3",
    "promise-polyfill": "7.1.2",
    "serve": "^10.1.1",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.0.0",
    "ava": "1.0.0-rc.2",
    "babel-plugin-istanbul": "^5.1.0",
    "browser-env": "^3.2.5",
    "istanbul-instrumenter-loader": "^3.0.1",
    "node-sass": "^4.10.0",
    "nyc": "^13.1.0",
    "require-extension-hooks": "^0.3.3",
    "require-extension-hooks-babel": "^0.1.1",
    "require-extension-hooks-vue": "^1.1.0",
    "sass-loader": "^7.1.0",
    "tyu": "1.0.6",
    "vue-devtools": "^5.0.0-beta.1",
    "vue-test-utils": "^1.0.0-beta.11"
  },
  "ava": {
    "require": [
      "./test/helpers/setup.js",
      "@babel/polyfill"
    ]
  },
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "lines": 0,
    "statements": 0,
    "functions": 0,
    "branches": 9,
    "ignore-class-method": [
      "VueRouter",
      "RouterLink"
    ],
    "include": [
      "src/**/*.{js,vue}"
    ],
    "exclude": [
      "src/*.js",
      "src/index.js",
      "src/**/*.test.js"
    ],
    "require": [
      "./test/helpers/setup.js"
    ],
    "cache": true
  }
}
